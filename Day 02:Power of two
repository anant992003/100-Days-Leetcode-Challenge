class PowerOfTwo {
   PowerOfTwo() {
   }

   public static void main(String[] var0) {
      int var1 = 64;
      if (var1 <= 0) {
         System.out.println("false");
      } else if (var1 == 1) {
         System.out.println("true");
      } else {
         while(var1 % 2 == 0) {
            var1 /= 2;
         }

         if (var1 == 1) {
            System.out.println("true");
         } else {
            System.out.println("false");
         }
      }

   }
}
