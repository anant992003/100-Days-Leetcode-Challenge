import java.util.Scanner;
class pallindrome {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.print("please enter a number:");
        int n=in.nextInt();
        int fin=0;
        
        int x=n;
         
        while(n>0){

            int temp=n%10;
            fin=fin*10+temp;
            n=n/10;


        }
        if(fin==x){
            System.out.println("pallindrome");
        }
        else{
            System.out.println("not a pallindrome");
        }
    }
    
}
